{
  "name": "@super_raptor911/render3d",
  "version": "0.0.11",
  "description": "A simple 3D graphics framework wsing webgl",
  "type": "module",
  "main": "./lib/es6/src/index.js",
  "types": "./lib/es6/src/index.d.ts",
  "module": "./lib/es5/src/index.js",
  "repository": "https://github.com/superRaptor911/R3D.git",
  "engines": {
    "node": ">= 16.13"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "tslib": "~2.4",
    "webgl-gltf": "^0.3.3",
    "webgl-obj-loader": "^2.0.8"
  },
  "devDependencies": {
    "@types/jest": "~29.1",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "~5.39",
    "@typescript-eslint/parser": "~5.39",
    "esbuild": "^0.15.14",
    "esbuild-plugin-mxn-copy": "^1.0.1",
    "eslint": "~8.24",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "npm-watch": "^0.11.0",
    "prettier": "~2.7",
    "rimraf": "~3.0",
    "serve": "^14.1.2",
    "tsutils": "~3.21",
    "typescript": "~4.8"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,css",
      "quiet": true
    }
  },
  "scripts": {
    "start": "serve",
    "dev": "live-server --watch=build",
    "clean": "rimraf coverage build tmp",
    "build": "node esbuild.config.js",
    "lint": "eslint . --ext .ts --ext .mts",
    "prettier": "prettier --config .prettierrc --write .",
    "compile": "rm -rf dist/lib && tsc && tsc --build tsconfig.es5.json",
    "watch": "npm-watch",
    "pub": "npm publish --access=public"
  },
  "author": "super_raptor911",
  "license": "Apache-2.0",
  "volta": {
    "node": "16.17.0"
  }
}
